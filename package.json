{
	"name": "terraform-doc-links",
	"displayName": "Terraform documentation links",
	"description": "Open Terraform documentation page of resources and data sources",
	"version": "0.0.1",
	"publisher": "EvopsLimited",
	"repository": {
		"type": "git",
		"url": "https://github.com/eu-evops/terraform-documentation-links"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"license": "BSD-3-Clause",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:terraform"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "terraformDocOpener.visitDocumentation",
				"title": "Open the Terraform documentation"
			}
		]
	},
	"galleryBanner": {
		"color": "#282828",
		"theme": "dark"
	},
	"scripts": {
		"vscode:prepublish": "npm run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.6.1"
	},
	"dependencies": {
		"vsce": "^1.73.0"
	},
	"__metadata": {
		"id": "050dad05-e01d-46ea-8061-8c945b7ee72f",
		"publisherDisplayName": "Stefan van der Haven",
		"publisherId": "0a266868-0dc7-4407-96f8-7838fbefa269",
		"isPreReleaseVersion": false
	}
}